{
  "manifest_version": 3,
  "name": "Suk Tab",
  "version": "1.0",
  "description": "A chrome extension that replaces the new tab page with a google search box and auto complete feature.",
  "chrome_url_overrides": {
    "newtab": "dist/index.html"
  },
  "host_permissions": [
    "https://www.google.com/complete/search*",
    "http://www.google.com/s2/favicons*",
    "https://t3.gstatic.com/faviconV2*"
  ],
  "permissions": ["bookmarks" ]
}
